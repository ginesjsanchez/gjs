<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

	<parent>
		<groupId>org.gjs.java</groupId>
		<artifactId>gjs-java-test-parent</artifactId>
		<version>1.0.0-SNAPSHOT</version>    
		<relativePath>../gjs-java-test-parent</relativePath>
	</parent>	

    <groupId>org.gjs.java</groupId>
    <artifactId>gjs-java-sb-test-parent</artifactId>
    <version>1.0.0-SNAPSHOT</version>
	<packaging>pom</packaging>

	<name>GJS common Java test parent for Spring Boot projects</name>
	<description>
	</description>

	<properties>
		<springboot.version>3.5.7</springboot.version>
	</properties>
	
    <dependencyManagement>
        <dependencies>
			<dependency>
				<groupId>org.gjs.java</groupId>
				<artifactId>gjs-java-web-parent</artifactId>
				<version>1.0.0-SNAPSHOT</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
			
			<!-- Spring Boot -->
			<dependency>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-dependencies</artifactId>
				<version>${springboot.version}</version> 
				<type>pom</type>
				<scope>import</scope>
			</dependency>
			<dependency>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-starter-test</artifactId>
				<version>${spring-boot.version}</version>
				<exclusions>
					<exclusion>
						<groupId>org.springframework.boot</groupId>
						<artifactId>spring-boot-starter-logging</artifactId>
					</exclusion>
				</exclusions>
			</dependency>
		</dependencies>
    </dependencyManagement>

    <dependencies>
		<!-- Spring Boot -->
		<dependency>
			<groupId>org.springframework.security</groupId>
			<artifactId>spring-security-test</artifactId>
		</dependency>
	</dependencies>

	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.springframework.boot</groupId>
					<artifactId>spring-boot-maven-plugin</artifactId>
					<version>${springboot.version}</version> 
				</plugin>
			</plugins>
		</pluginManagement>		

		<extensions>
		</extensions>

		<plugins>
		</plugins>
	</build>

</project>